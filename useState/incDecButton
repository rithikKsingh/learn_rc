import React, { useState } from "react";

function App() {
  const [count, setCount] = useState(0);
  function incCount() {
    setCount(count + 1);
  }
  function decCount() {
    setCount(count - 1);
  }
  return (
    <div>
      <h1>{count}</h1>
      <button onClick={incCount}>+</button>
      <button onClick={decCount}>-</button>
    </div>
  );
}

export default App;
